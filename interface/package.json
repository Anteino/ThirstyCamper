{
  "name": "go-sharing-dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://go-sharing.github.io/frontend-dashboard",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mapbox/mapbox-gl-draw": "^1.3.0",
    "@mui/material": "^5.10.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-ioc": "^1.0.0",
    "react-map-gl": "^7.0.19",
    "react-router-dom": "^5.3.3",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
